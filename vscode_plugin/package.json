{
  "name": "spp-syntax-extension",
  "displayName": "C++ Sugar Syntax Extension",
  "description": "Syntax rules for the C++ Sugar",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mubashar-javed/quick-snippets"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "spp",
      "aliases": ["spp"],
      "extensions": [".spp"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "icon.png",
        "dark": "icon.png"
      }
    }],
    "grammars": [{
      "language": "spp",
      "scopeName": "source.spp",
      "path": "./syntaxes/spp.tmLanguage.json"
    }],
    "icons": {
      
    }
  }
}
